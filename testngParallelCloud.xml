<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parlles Test Suite" parallel="tests" thread-count="6">
	<listeners>
		<listener class-name="resources.Listeners" />
	</listeners>
	<test name="Ecommerce Test">
		<classes>
			<class name="ecommerceApp.Ecommerce_tc_6_Cloud" />
		</classes>
	</test>
	<test name="API Demo Test" parallel="classes" thread-count="2">
		<classes>
			<class name="apiDemoApp.ApiDemoTest_Cloud" />
		</classes>
	</test>
</suite>
<!-- thread-count=6 means run 6 tests together -->
